      .menu.menu-column.menu-title-gray-800.menu-state-title-primary.menu-state-icon-primary.menu-state-bullet-primary.menu-arrow-gray-500(id='#kt_aside_menu' data-kt-menu='true' data-kt-menu-expand='false')
        .menu-item
          a.menu-link(href='/courses')
            span.menu-icon
              include ../svg/catalog
            span.menu-title Catalog
        if locals.me
          .menu-item
            a.menu-link(href='/dashboard')
              span.menu-icon
                include ../svg/dashboard
              span.menu-title Tableau de Bord
          .menu-item
            .menu-content.pt-8.pb-2
              span.menu-section.text-muted.text-uppercase.fs-8.ls-1 Navigation
          .menu-item
            a.menu-link(href='/leads')
              span.menu-icon
                include ../svg/calendar
              span.menu-title Rendez-Vous
          .menu-item
            a.menu-link(href='/agents')
              span.menu-icon
                include ../svg/user
              span.menu-title Agents
          .menu-item
            a.menu-link(href='/customers')
              span.menu-icon
                include ../svg/suitcase
              span.menu-title Clients &amp; Prospects
          .menu-item
            a.menu-link(href='/settings')
              span.menu-icon
                include ../svg/switch
              span.menu-title Parametres
          .menu-item
            .menu-content
              .separator.mx-1.my-4
