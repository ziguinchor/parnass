#kt_aside.aside.aside-dark.aside-hoverable(data-kt-drawer='true' data-kt-drawer-name='aside' data-kt-drawer-activate='{default: true, lg: false}' data-kt-drawer-overlay='true' data-kt-drawer-width="{default:'200px', '300px': '250px'}" data-kt-drawer-direction='start' data-kt-drawer-toggle='#kt_aside_mobile_toggle')
  #kt_aside_logo.aside-logo.flex-column-auto
    a(href='/')
      img.h-55px.logo(alt='Logo' src='/assets/media/logos/logo.png')
    #kt_aside_toggle.btn.btn-icon.w-auto.px-0.btn-active-color-primary.aside-toggle(data-kt-toggle='true' data-kt-toggle-state='active' data-kt-toggle-target='body' data-kt-toggle-name='aside-minimize')
      include ../svg/double-left-arrows
